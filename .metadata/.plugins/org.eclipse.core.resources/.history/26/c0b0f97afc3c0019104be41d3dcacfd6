#%RAML 1.0
baseUri: https://anypoint.mulesoft.com/mocking/api/v1/links/afeae993-e0d9-4064-8cd8-91f2c119e774/ # 
version: v1
title: American Flights API

types:
  AmericanFlight: !include exchange_modules/68ef9520-24e9-4cf2-b2f5-620025690913/training-american-flight-data-type/1.0.1/AmericanFlightDataType.raml

/flights:
   post:
    body:
      application/json:
        type: AmericanFlight
        examples:
          ex1: !include example/AmericanFlightExample.raml
    responses:
      201:
        body:
          application/json:
            examples:
              message: Flight added (but not really)
        

   get:
    queryParameters:
      destination:
        required: false
        enum:
          - SFO
          - LAX
          - CLE
    responses:
      200:
        body:
          application/json:
            type: AmericanFlight[]
            examples: 
              ex1: !include exchange_modules/68ef9520-24e9-4cf2-b2f5-620025690913/training-american-flights-example/1.0.1/AmericanFlightsExample.raml
/{ID}:
  get:
    responses:
      200:
        body:
          application/json:
            type: AmericanFlight
            examples: !include example/AmericanFlightExample.raml

              
              


              


